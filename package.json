{
  "name": "link-walker",
  "version": "0.0.0",
  "description": "CLI library for scraping links",
  "author": "mbs-code",
  "private": true,
  "bin": {
    "link-walker": "./bin/run"
  },
  "main": "dist/index.js",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^1",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.0.1",
    "@prisma/client": "^3.9.2",
    "cheerio": "^1.0.0-rc.10",
    "got": "^11",
    "joi": "^17.6.0",
    "js-yaml": "^4.1.0",
    "log4js": "^6.4.1",
    "pretty-bytes": "5",
    "prisma": "^3.9.2",
    "sqlite3": "^5.0.2"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^17.0.18",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.2",
    "eslint-config-prettier": "^8.3.0",
    "globby": "^11",
    "husky": "^7.0.0",
    "lint-staged": "^12.3.4",
    "oclif": "^2",
    "prettier": "^2.5.1",
    "shx": "^0.3.3",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "oclif": {
    "bin": "link-walker",
    "dirname": "link-walker",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "prettier": "prettier **/*.ts --check",
    "fmt": "eslint --ext .ts --fix && prettier **/*.ts --write",
    "lint-staged": "lint-staged",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "version": "oclif readme && git add README.md",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "lint",
      "prettier"
    ]
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "types": "dist/index.d.ts"
}
